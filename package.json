  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "babel-loader": "^9.2.1",
    "electron": "^32.1.2",
    "electron-builder": "^25.0.5",
    "electron-is-dev": "^3.0.1",
    "html-webpack-plugin": "^5.6.0",
    "webpack": "^5.94.0",
